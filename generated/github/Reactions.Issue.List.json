{
  "moduleName": "GitHub / Reactions / Issue / List",
  "version": "0.0.0",
  "description": "List the reactions to an [issue](https://docs.github.com/rest/issues/issues#get-an-issue).",
  "keywords": [],
  "cacheMode": "always",
  "evalMode": "manual",
  "params": {
    "accessToken": {
      "schema": {
        "type": "any",
        "description": "GitHub Access Token"
      },
      "advanced": false,
      "hideValue": true
    },
    "owner": {
      "schema": {
        "type": "string",
        "description": "The account owner of the repository. The name is not case sensitive."
      },
      "advanced": false,
      "hideValue": false
    },
    "repo": {
      "schema": {
        "type": "string",
        "description": "The name of the repository without the `.git` extension. The name is not case sensitive."
      },
      "advanced": false,
      "hideValue": false
    },
    "issueNumber": {
      "schema": {
        "type": "number",
        "description": "The number that identifies the issue."
      },
      "advanced": false,
      "hideValue": false
    },
    "content": {
      "schema": {
        "type": "string",
        "enum": [
          "+1",
          "-1",
          "laugh",
          "confused",
          "heart",
          "hooray",
          "rocket",
          "eyes"
        ],
        "optional": true,
        "description": "Returns a single [reaction type](https://docs.github.com/rest/reactions/reactions#about-reactions). Omit this parameter to list all reactions to an issue."
      },
      "advanced": true,
      "hideValue": false
    },
    "perPage": {
      "schema": {
        "type": "number",
        "default": "30",
        "optional": true,
        "description": "The number of results per page (max 100). For more information, see \"[Using pagination in the REST API](https://docs.github.com/rest/using-the-rest-api/using-pagination-in-the-rest-api).\""
      },
      "advanced": true,
      "hideValue": false
    },
    "page": {
      "schema": {
        "type": "number",
        "default": "1",
        "optional": true,
        "description": "The page number of the results to fetch. For more information, see \"[Using pagination in the REST API](https://docs.github.com/rest/using-the-rest-api/using-pagination-in-the-rest-api).\""
      },
      "advanced": true,
      "hideValue": false
    }
  },
  "result": {
    "schema": {
      "type": "any"
    },
    "async": true
  },
  "attributes": {
