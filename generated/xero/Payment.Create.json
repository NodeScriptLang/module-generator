      },
      "advanced": false,
      "hideValue": false
    },
    "idempotencyKey": {
      "schema": {
        "type": "string",
        "optional": true,
        "description": "This allows you to safely retry requests without the risk of duplicate processing. 128 character max."
      },
      "advanced": true,
      "hideValue": false
    },
    "invoice": {
      "schema": {
