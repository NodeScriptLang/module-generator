      "schema": {
        "type": "string",
        "optional": true,
        "description": "UTC timestamp of last update to the payment"
      },
      "advanced": true,
      "hideValue": false
    },
    "paymentId": {
      "schema": {
        "type": "string",
        "optional": true,
        "description": "The Xero identifier for an Payment e.g. 297c2dc5-cc47-4afd-8ec8-74990b8761e9"
      },
      "advanced": true,
      "hideValue": false
    },
    "batchPaymentId": {
      "schema": {
        "type": "string",
        "optional": true,
        "description": "Present if the payment was created as part of a batch."
      },
      "advanced": true,
      "hideValue": false
    },
    "bankAccountNumber": {
      "schema": {
        "type": "string",
        "optional": true,
        "description": "The suppliers bank account number the payment is being made to"
      },
      "advanced": true,
      "hideValue": false
    },
    "particulars": {
      "schema": {
        "type": "string",
        "optional": true,
        "description": "The suppliers bank account number the payment is being made to"
      },
      "advanced": true,
      "hideValue": false
    },
    "details": {
      "schema": {
        "type": "string",
