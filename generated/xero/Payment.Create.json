    "prepayment": {
      "schema": {
        "type": "any",
        "optional": true,
        "description": ""
      },
      "advanced": true,
      "hideValue": true
    },
    "overpayment": {
      "schema": {
        "type": "any",
        "optional": true,
        "description": ""
      },
      "advanced": true,
      "hideValue": true
    },
    "invoiceNumber": {
      "schema": {
        "type": "string",
        "optional": true,
        "description": "Number of invoice or credit note you are applying payment to e.g.INV-4003"
      },
      "advanced": true,
      "hideValue": false
    },
    "creditNoteNumber": {
      "schema": {
        "type": "string",
        "optional": true,
        "description": "Number of invoice or credit note you are applying payment to e.g. INV-4003"
      },
      "advanced": true,
      "hideValue": false
    },
    "batchPayment": {
      "schema": {
        "type": "any",
        "optional": true,
        "description": ""
